{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@services": [
        "services"
      ],
      "@services/*": [
        "services/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@components": [
        "components"
      ],
      "@assets/*": [
        "assets/*"
      ]
    }
  },
  "include": [
    "src",
    "test"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}