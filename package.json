  "scripts": {
    "prepare": "concurrently npm:install:* && husky install",
    "install:server": "cd server && npm install",
    "install:client": "cd client && npm install",
    "dev": "concurrently npm:dev:*",
    "dev:server": "npm run build:client && cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "start": "npm run start:server",
    "start:server": "cd server && npm start",
    "build": "concurrently npm:build:*",
    "build:server": "cd server && npm run build",
    "build:client": "cd client && npm run build",
    "lint:fix": "concurrently npm:lint:fix:*",
    "lint:fix:client": "cd client && npm run lint:fix",
    "lint:fix:server": "cd server && npm run lint:fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.4.0"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3"
  }
}
